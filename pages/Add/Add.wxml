  <view class='tab_view' >
    <view class="tab_list" >
      <view class='tab_item' catchtap='toTabUrl' data-tab-url='SignIn/SignIn' data-show-id='1'>
        <image src="/images/signIn.png" class="tab_img" />
        <text>每日签到</text>
      </view>
      <view class='tab_item' catchtap='toTabUrl' data-tab-url='InitiateActivity/InitiateActivity' data-show-id='2'>
        <image  src="/images/renwu.png" class="tab_img" />
