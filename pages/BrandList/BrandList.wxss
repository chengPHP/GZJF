/* pages/BrandList/BrandList.wxss */
.list_title{
  /* border: 1px solid red; */
  padding: 10rpx 20rpx;
}
.list_title .title_name{
  border-left: 10rpx solid #4395FF;
  font-size: 28rpx;
  padding-left: 10rpx;
  color: #333;
}
.list_item{
  /* border: 1px solid red; */
  color: #333;
  padding: 20rpx;
  border-bottom: 1rpx solid #efefef;
}
.list_item .num{
  display: inline-block;
  padding: 10rpx 20rpx;
}
.list_item .brand_name{
  display: inline-block;
  padding: 10rpx 20rpx;
  font-size: 32rpx;
  width: 60%;
  /* 文字溢出显示省略号 */
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.list_item .item_right{
  float: right;
  margin-top: 10rpx;
  font-size: 32rpx;
}
.list_item .item_right .right_number{
  color: red;
}